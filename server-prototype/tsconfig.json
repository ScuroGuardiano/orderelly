{
  "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "sourceMap": false,
      "outDir": "target",
      "noImplicitAny": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "allowUnreachableCode": false,
      "alwaysStrict": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "incremental": true
  },
  "compileOnSave": true
}
